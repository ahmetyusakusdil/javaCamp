package oopIntro;

public class Main {

	public static void main(String[] args) {
		
		Product product1 = new Product(); //Örnek Oluþturma , intance oluþturma , referance oluþturma
		product1.id=1;
		product1.name="Lenovo";
		product1.unitPrice=25;
		product1.detail="16 GB Ram";
		
		
		
		
		Product[] products = {product1};
		System.out.println(products.length);
	}

}
