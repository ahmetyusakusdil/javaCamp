package oopIntro;

public class Main {

	public static void main(String[] args) {
		
		Product product1 = new Product(); //Örnek Oluþturma , intance oluþturma , referance oluþturma
		product1.id=1;
		product1.name="Lenovo";
		product1.unitPrice=25;
		product1.detail="16 GB Ram";
		
	
		Product product2 = new Product(); //Örnek Oluþturma , intance oluþturma , referance oluþturma
		product2.id=2;
		product2.name="Acer";
		product2.unitPrice=25;
		product2.detail="16 GB Ram";
		
	
		Product product3 = new Product(); //Örnek Oluþturma , intance oluþturma , referance oluþturma
		product3.id=3;
		product3.name="Asus";
		product3.unitPrice=25;
		product3.detail="16 GB Ram";
		
	
		
		
		Product[] products = {product1,product2,product3};
		System.out.println(products.length);
	}

}
